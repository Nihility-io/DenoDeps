{
	"name": "@nihility-io/deno-deps",
	"version": "1.1.7",
	"exports": "./cmd.ts",
	"lock": true,
	"lint": {
		"rules": {
			"tags": ["recommended", "jsr"],
			"include": [
				"jsx-button-has-type",
				"camelcase",
				"default-param-last",
				"eqeqeq",
				"explicit-function-return-type",
				"explicit-module-boundary-types",
				"no-const-assign",
				"no-eval",
				"no-external-import",
				"no-inferrable-types",
				"no-self-compare",
				"no-sparse-arrays",
				"no-sync-fn-in-async-fn",
				"no-throw-literal",
				"no-undef",
				"no-useless-rename",
				"single-var-declarator"
			],
			"exclude": [
				"verbatim-module-syntax"
			]
		}
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"semiColons": false,
		"singleQuote": false,
		"proseWrap": "preserve"
	},
	"imports": {
		"@std/cli": "jsr:@std/cli@^1.0.17",
		"@std/fs": "jsr:@std/fs@^1.0.17",
		"@std/jsonc": "jsr:@std/jsonc@^1.0.2",
		"@std/path": "jsr:@std/path@^1.0.9",
		"@std/semver": "jsr:@std/semver@^1.0.5",
		"@std/toml": "jsr:@std/toml@^1.0.6",
		"@std/yaml": "jsr:@std/yaml@^1.0.6",
		"zod": "npm:zod@^3.25.28"
	}
}
